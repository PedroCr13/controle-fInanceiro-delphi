  inherited qryConsulta: TSQLQuery
    Params = <
      item
        DataType = ftInteger
        Name = 'USUARIO'
        ParamType = ptInput
        Value = 0
      end>
    SQL.Strings = (
      'SELECT I.CODIGO,'
      '       I.USUARIO,'
      '       I.AGENTE,'
      '       A.NOME,'
      '       I.TP_APLICACAO,'
      '       TA.DESCRICAO AS "APLICACAO",'
      '       I.DATA_APLIC,'
      '       I.DATA_VENC,'
      '       I.LIQUIDEZ,'
      '       L.DESCRICAO AS "DESCLIQUIDEZ",'
      '       I.INDICE,'
      '       IX.DESCRICAO AS "INDEXADOR",'
      '       C.CODIGO AS "COD_CATEGORIA",'
      '       C.DESCRICAO AS "CATEGORIA",'
      '       I.VALOR_APLICADO,'
      '       I.TAXA_REMUNERACAO,'
      '       I.ATIVO'
      'FROM TB_INVESTIMENTOS I'
      'INNER JOIN TB_AGENTES A ON (I.AGENTE = A.CODIGO)'
      'INNER JOIN TB_TIPOAPLICACOES TA ON (I.TP_APLICACAO = TA.CODIGO)'
      'INNER JOIN TB_TIPOLIQUIDEZ L ON (I.LIQUIDEZ = L.CODIGO)'
      'INNER JOIN TB_INDEXADOR IX ON (I.INDICE = IX.CODIGO)'
      'INNER JOIN TB_CATEGORIA_APLICACAO C ON (C.CODIGO = TA.CATEGORIA)'
      'WHERE I.USUARIO =:USUARIO'
      'AND I.ATIVO = '#39'S'#39
      'ORDER BY I.DATA_APLIC')
    object qryConsultaCODIGO: TIntegerField
      FieldName = 'CODIGO'
      Required = True
