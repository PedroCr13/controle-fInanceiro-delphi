  inherited qryConsulta: TSQLQuery
    Params = <
      item
        DataType = ftInteger
        Name = 'USUARIO'
        ParamType = ptInput
        Value = 0
      end>
    SQL.Strings = (
      'SELECT C.CODIGO,'
      '       A.NOME,'
      '       C.CONTA,'
      '       C.COD_USUARIO'
      'FROM TB_CONTAS C'
      'INNER JOIN tb_agentes A ON (C.BANCO = A.CODIGO)'
      'WHERE C.CODIGO > 0'
      'AND C.COD_USUARIO =:USUARIO'
      'ORDER BY A.NOME')
